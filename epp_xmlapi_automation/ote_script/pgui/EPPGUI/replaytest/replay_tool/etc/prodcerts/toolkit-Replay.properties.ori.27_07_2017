#OTE
epp.server.hostname=ote.registry.qa
epp.server.port=700


epp.client.clID=qdr_epp
epp.client.password=qdr16!^QDR
epp.client.session.count.max=6
epp.client.options.version=1.0
epp.client.options.lang=en

ssl.keystore.location=qdr_epp-ks.jks
ssl.keystore.type=jks
ssl.keystore.pass=qdr16QDR!^
